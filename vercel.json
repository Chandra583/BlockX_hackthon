{
  "version": 2,
  "buildCommand": "echo 'No build required - serverless functions'",
  "outputDirectory": "backend",
  "builds": [
    {
      "src": "backend/api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/api/index.ts"
    }
  ]
}

